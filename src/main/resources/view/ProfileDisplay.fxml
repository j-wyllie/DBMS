<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="1200.0"
        xmlns="http://javafx.com/javafx/8"
        fx:controller="odms.view.profile.ProfileDisplayControllerTODO">
    <children>
        <GridPane AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="-1.0" AnchorPane.rightAnchor="-1.0"
                AnchorPane.topAnchor="-1.0" prefHeight="800.0"
                prefWidth="1200.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="150.0"
                        vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TabPane GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="3"
                        prefHeight="200.0" prefWidth="1202.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab onSelectionChanged="#onTabGeneralSelected" text="General">
                            <fx:include fx:id="profileGeneralTab" source="ProfileGeneralTab.fxml"
                                GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
                        </Tab>

                        <Tab onSelectionChanged="#onTabMedicalSelected" text="Medical">
                            <fx:include fx:id="profileMedicalTab" source="ProfileMedicalTab.fxml"
                                GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
                        </Tab>
                        <Tab onSelectionChanged="#onTabOrgansSelected" text="Organs">
                            <fx:include GridPane.halignment="CENTER"
                                GridPane.valignment="CENTER"
                                source="ProfileOrganOverview.fxml" fx:id="profileOrganOverview"/>
                        </Tab>
                        <Tab onSelectionChanged="#onTabHistorySelected" text="History">
                            <fx:include fx:id="profileHistoryTab" source="ProfileHistoryTab.fxml" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        </Tab>
                        <Tab onSelectionChanged="#onTabMedicationsSelected" text="Medications">
                            <fx:include fx:id="profileMedicationsTab" source="ProfileMedicationsTab.fxml" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        </Tab>
                        <Tab onSelectionChanged="#onTabMedicalHistorySelected" text="MedicalHistory">
                            <fx:include fx:id="profileMedicalHistoryTab" source="ProfileMedicalHistoryTab.fxml" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        </Tab>
                        <Tab onSelectionChanged="#onTabProceduresSelected" text="Procedures">
                            <fx:include fx:id="profileProceduresTab" source="ProfileProceduresTab.fxml" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        </Tab>
                        <Tab text="Procedures" fx:id="procedureTab">
                            <content>

                            </content>

                        </Tab>
                    </tabs>
                </TabPane>
                <GridPane GridPane.columnSpan="2" prefHeight="146.0" prefWidth="1203.0"
                        stylesheets="@../styles/Common.css">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1141.0" minWidth="10.0"
                                prefWidth="907.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0"
                                prefWidth="168.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="15.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="25.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="28.0" minHeight="10.0" prefHeight="22.0"
                                vgrow="NEVER"/>
                        <RowConstraints maxHeight="74.0" minHeight="10.0" prefHeight="45.0"
                                vgrow="NEVER"/>
                        <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="35.0"
                                vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="36.0"
                                vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label GridPane.columnSpan="2" GridPane.rowIndex="1" prefHeight="52.0"
                                prefWidth="972.0" styleClass="banner-text-header"
                                stylesheets="@../styles/Profile.css" text="Brooke Anne Rakowitz" textFill="WHITE"
                                fx:id="donorFullNameLabel">
                            <font>
                                <Font name="System Bold" size="40.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets bottom="10.0" left="40.0"/>
                            </GridPane.margin>
                        </Label>
                        <Label GridPane.rowIndex="2" prefHeight="40.0" prefWidth="1141.0"
                                styleClass="banner-text" stylesheets="@../styles/Profile.css"
                                text="Donor Status: " textFill="WHITE" fx:id="donorStatusLabel">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets bottom="10.0" left="40.0"/>
                            </GridPane.margin>
                        </Label>
                        <Button style="-fx-background-image: url(icons/logout.png);" GridPane.columnIndex="3" GridPane.halignment="RIGHT"
                                GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER"
                                layoutX="1135.0" layoutY="10.0"
                                maxHeight="-Infinity" maxWidth="-Infinity"
                                minHeight="-Infinity"
                                minWidth="-Infinity"
                                mnemonicParsing="false" onAction="#handleLogoutButtonClicked"
                                prefHeight="45.0" prefWidth="45.0"
                                styleClass="button-logout" stylesheets="@../styles/Profile.css"
                                fx:id="logoutButton">
                            <GridPane.margin>
                                <Insets/>
                            </GridPane.margin>
                        </Button>
                        <Label GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2"
                                prefHeight="40.0" prefWidth="159.0"
                                styleClass="banner-text" stylesheets="@../styles/Profile.css" text="User ID : "
                                textFill="WHITE" fx:id="userIdLabel">
                            <font>
                                <Font size="28.0"/>
                            </font>
                        </Label>
                        <Label GridPane.rowIndex="3" layoutX="60.0" layoutY="86.0"
                                prefHeight="40.0" prefWidth="1141.0" styleClass="banner-text"
                                stylesheets="@../styles/Profile.css" text="Receiver Status: "
                                textFill="WHITE" fx:id="receiverStatusLabel">
                            <font>
                                <Font size="28.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets bottom="10.0" left="40.0"/>
                            </GridPane.margin>
                        </Label>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <styleClass>
                        <String fx:value="banner"/>
                        <String fx:value="banner-default"/>
                    </styleClass>
                </GridPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
