<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="400.0"
        xmlns="http://javafx.com/javafx/8"
        fx:controller="odms.controller.medication.MedicationHistoryTODO">
    <children>
        <GridPane AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="-1.0"
                AnchorPane.rightAnchor="-1.0" AnchorPane.topAnchor="-1.0"
                maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="100.0" minHeight="10.0" prefHeight="30.0"
                        vgrow="ALWAYS"/>
                <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="ALWAYS"/>
            </rowConstraints>
            <children>
                <Label GridPane.columnSpan="3" GridPane.halignment="CENTER"
                        GridPane.valignment="TOP" maxHeight="1.7976931348623157E308"
                        maxWidth="1.7976931348623157E308" stylesheets="@../styles/Common.css"
                        text="Medication History" fx:id="bannerLabel">
                    <styleClass>
                        <String fx:value="banner-default"/>
                        <String fx:value="banner"/>
                    </styleClass>
                </Label>
                <GridPane GridPane.rowIndex="1" alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints maxWidth="1.7976931348623157E308" minWidth="200.0"
                                prefWidth="440.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS"/>
                        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="35.0"
                                vgrow="ALWAYS"/>
                    </rowConstraints>
                    <opaqueInsets>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </opaqueInsets>
                    <children>
                        <ListView prefHeight="200.0" prefWidth="200.0"
                                fx:id="viewMedicationHistory"/>
                        <Button GridPane.halignment="CENTER" GridPane.rowIndex="1"
                                GridPane.valignment="CENTER" maxWidth="1.7976931348623157E308"
                                mnemonicParsing="false" onAction="#handleCloseButtonClicked" text="Close"
                                fx:id="buttonClose">
                            <GridPane.margin>
                                <Insets top="10.0"/>
                            </GridPane.margin>
                        </Button>
                    </children>
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </GridPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
