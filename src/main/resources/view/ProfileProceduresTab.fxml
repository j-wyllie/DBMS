<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1"
  xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <GridPane AnchorPane.bottomAnchor="-1.0"
        AnchorPane.leftAnchor="-1.0"
        AnchorPane.rightAnchor="-1.0"
        AnchorPane.topAnchor="-1.0">
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
            prefWidth="100.0"/>
          <ColumnConstraints hgrow="NEVER"
            maxWidth="1.7976931348623157E308"
            minWidth="10.0" prefWidth="170.0"/>
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0"
            prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="1.7976931348623157E308"
            minHeight="10.0" prefHeight="40.0"
            vgrow="NEVER"/>
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
          <Insets bottom="10.0" left="10.0" right="10.0"
            top="10.0"/>
        </padding>
        <children>
          <Text GridPane.halignment="CENTER" GridPane.valignment="CENTER"
            strokeType="OUTSIDE"
            strokeWidth="0.0"
            text="Pending Procedures">
            <font>
              <Font name="Arial Bold" size="25.0"/>
            </font>
          </Text>
          <Text GridPane.columnIndex="2" GridPane.halignment="CENTER"
            GridPane.valignment="CENTER"
            strokeType="OUTSIDE"
            strokeWidth="0.0"
            text="Previous Procedures">
            <font>
              <Font name="Arial Bold" size="25.0"/>
            </font>
          </Text>
          <TableView GridPane.rowIndex="1"
            prefHeight="458.0" prefWidth="455.0"
            fx:id="pendingProcedureTable">
            <columns>
              <TableColumn prefWidth="138.0"
                text="Summary" fx:id="pendingSummaryColumn"/>
              <TableColumn prefWidth="140.0"
                text="Date of Procedure"
                fx:id="pendingDateColumn"/>
              <TableColumn prefWidth="176.0"
                text="Affects" fx:id="pendingAffectsColumn"/>
            </columns>
            <columnResizePolicy>
              <TableView
                fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
          </TableView>
          <TableView GridPane.columnIndex="2"
            GridPane.rowIndex="1" prefHeight="453.0"
            prefWidth="447.0"
            fx:id="previousProcedureTable">
            <columns>
              <TableColumn prefWidth="125.0"
                text="Summary" fx:id="previousSummaryColumn"/>
              <TableColumn prefWidth="142.0"
                text="Date of Procedure"
                fx:id="previousDateColumn"/>
              <TableColumn prefWidth="178.0"
                text="Affects" fx:id="previousAffectsColumn"/>
            </columns>
            <columnResizePolicy>
              <TableView
                fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
          </TableView>
          <GridPane GridPane.columnIndex="1"
            GridPane.rowIndex="1">
            <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES"
                minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
              <RowConstraints minHeight="10.0"
                prefHeight="30.0"
                vgrow="SOMETIMES"/>
              <RowConstraints minHeight="10.0"
                prefHeight="30.0"
                vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
              <Button GridPane.hgrow="ALWAYS"
                GridPane.valignment="BOTTOM"
                maxWidth="1.7976931348623157E308"
                mnemonicParsing="false"
                onAction="#handleAddProcedureButtonClicked"
                text="Add new Procedure"
                fx:id="addNewProcedureButton">
                <GridPane.margin>
                  <Insets bottom="10.0"/>
                </GridPane.margin>
              </Button>
              <Button GridPane.hgrow="ALWAYS"
                GridPane.rowIndex="1"
                GridPane.valignment="TOP"
                maxWidth="1.7976931348623157E308"
                mnemonicParsing="false"
                onAction="#handleDeleteProcedureButtonClicked"
                text="Delete Procedure"
                fx:id="deleteProcedureButton">
                <GridPane.margin>
                  <Insets top="10.0"/>
                </GridPane.margin>
              </Button>
            </children>
            <padding>
              <Insets bottom="10.0" left="10.0"
                right="10.0" top="10.0"/>
            </padding>
          </GridPane>
        </children>
      </GridPane>
    </children>
  </AnchorPane>

