<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="370.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="odms.controller.AddProcedureController">
    <children>
        <TextField fx:id="summaryField" layoutX="12.0" layoutY="41.0" prefHeight="29.0" prefWidth="339.0" promptText="Summary of procedure" />
        <Button fx:id="addButton" layoutX="224.0" layoutY="214.0" mnemonicParsing="false" onAction="#handleAddButtonClicked" prefHeight="42.0" prefWidth="127.0" text="Add" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="18.0" />
        <TextField fx:id="dateOfProcedureField" layoutX="12.0" layoutY="82.0" prefHeight="29.0" prefWidth="339.0" promptText="Date of Procedure (dd-mm-yyyy)" />
        <TextField fx:id="descriptionField" layoutX="12.0" layoutY="122.0" prefHeight="29.0" prefWidth="339.0" promptText="Procedure description (optional)" />
        <Label fx:id="ProcedureDetailsLabel" layoutX="14.0" layoutY="14.0" text="Procedure Details:" />
        <Label fx:id="warningLabel" alignment="CENTER" layoutX="14.0" layoutY="188.0" prefHeight="15.0" prefWidth="339.0" text="Invalid Details" textAlignment="CENTER" textFill="RED" visible="false" />
      <Label fx:id="ProcedureDetailsLabel1" layoutX="14.0" layoutY="159.0" text="Affected Organs (+ctrl for muliple):" />
      <ListView fx:id="affectedOrgansListView" layoutX="14.0" layoutY="181.0" prefHeight="119.0" prefWidth="339.0" />
    </children>
</AnchorPane>
