<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="odms.view.user.TransplantWaitingList"
        prefHeight="400.0" prefWidth="600.0">
    <GridPane AnchorPane.bottomAnchor="-1.0"
            AnchorPane.leftAnchor="-1.0"
            AnchorPane.rightAnchor="-1.0"
            AnchorPane.topAnchor="-1.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                    prefWidth="100.0"/>
            <ColumnConstraints hgrow="NEVER" minWidth="10.0"
                    prefWidth="300.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="35.0"
                    vgrow="NEVER"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0"
                    vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0"
                    top="10.0"/>
        </padding>
        <children>
            <TextField GridPane.columnIndex="1"
                    GridPane.valignment="TOP"
                    onKeyReleased="#handleSearchWaitingList"
                    prefHeight="25.0"
                    prefWidth="274.0"
                    promptText="Enter name"/>
            <TableView GridPane.columnSpan="2"
                    GridPane.rowIndex="1" prefHeight="461.0"
                    prefWidth="1178.0"
                    fx:id="transplantTable"/>
            <Button GridPane.halignment="RIGHT"
                    GridPane.valignment="TOP"
                    alignment="CENTER"
                    mnemonicParsing="false" onAction="#refreshTable"
                    text="Refresh"
                    fx:id="refreshButton2">
                <GridPane.margin>
                    <Insets right="10.0"/>
                </GridPane.margin>
            </Button>
        </children>
    </GridPane>
</AnchorPane>
