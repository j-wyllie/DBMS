<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="580.0" prefWidth="600.0"
        xmlns="http://javafx.com/javafx/8"
        fx:controller="odms.controller.user.UserCreateController">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
                focusTraversable="true" layoutX="-2.0" layoutY="2.0"
                prefHeight="400.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0"/>
                <ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="10.0"
                        prefWidth="400.0"/>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0"/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="150.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label GridPane.columnSpan="3" GridPane.valignment="TOP" alignment="CENTER"
                        contentDisplay="CENTER" maxWidth="1.7976931348623157E308"
                        prefHeight="150.0" stylesheets="@../styles/Common.css" text="Please enter your details..."
                        textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Arial" size="30.0"/>
                    </font>
                    <styleClass>
                        <String fx:value="banner-default"/>
                        <String fx:value="banner"/>
                    </styleClass>
                </Label>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="CENTER"
                        layoutX="110.0" layoutY="243.0"
                        maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                        minWidth="-Infinity" promptText="Username"
                        styleClass="text-field" stylesheets="@../styles/ProfileCreate.css" fx:id="userUsernameField">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <PasswordField GridPane.columnIndex="1" GridPane.hgrow="ALWAYS"
                        GridPane.rowIndex="3" GridPane.valignment="CENTER" maxHeight="1.7976931348623157E308"
                        maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                        minWidth="-Infinity" promptText="Password" stylesheets="@../styles/ProfileCreate.css"
                        fx:id="userPasswordField">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </PasswordField>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="4" GridPane.valignment="CENTER"
                        prefHeight="55.0" prefWidth="400.0"
                        promptText="Name" styleClass="text-field"
                        stylesheets="@../styles/ProfileCreate.css" fx:id="userNameField">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <TextField GridPane.columnIndex="1" GridPane.rowIndex="5" GridPane.valignment="CENTER" layoutX="110.0"
                        layoutY="186.0" prefHeight="55.0" prefWidth="400.0"
                        promptText="Region" styleClass="text-field"
                        stylesheets="@../styles/ProfileCreate.css" fx:id="userRegionField">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <Button GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.hgrow="ALWAYS" GridPane.rowIndex="7"
                        GridPane.valignment="CENTER" alignment="CENTER" maxHeight="1.7976931348623157E308"
                        maxWidth="1.7976931348623157E308"
                        minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false"
                        onAction="#handleUserCreateAccountButtonClicked" styleClass="button-create-account" stylesheets="@../styles/ProfileCreate.css"
                        text="CREATE ACCOUNT" textFill="WHITE" fx:id="userCreateAccountButton">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Button>
                <ChoiceBox GridPane.columnIndex="1" GridPane.rowIndex="6" prefHeight="65.0"
                        prefWidth="484.0" fx:id="userTypeBox"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
