<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0"
        xmlns="http://javafx.com/javafx/8"
        fx:controller="odms.view.profile.ProcedureDetailed">
    <children>
        <GridPane AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="-1.0" AnchorPane.rightAnchor="-1.0"
                AnchorPane.topAnchor="-1.0" prefHeight="700.0"
                prefWidth="1200.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="228.0" minHeight="10.0" prefHeight="142.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="451.0" minHeight="10.0" prefHeight="188.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="284.0" minHeight="10.0" prefHeight="194.0"
                        vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="174.0" minHeight="10.0" prefHeight="174.0"
                        vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TilePane style="-fx-background-color: #1fa98c;" GridPane.columnSpan="2"
                        prefHeight="52.0" prefWidth="1200.0">
                    <children>
                        <GridPane prefHeight="116.0" prefWidth="803.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="517.0"
                                        minWidth="10.0" percentWidth="80.0" prefWidth="475.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="0.0"
                                        prefWidth="42.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="0.0"
                                        prefWidth="43.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="102.0" minWidth="0.0"
                                        prefWidth="40.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="27.0" minHeight="10.0" prefHeight="26.0"
                                        vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0"
                                        vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="52.0" minHeight="10.0" prefHeight="31.0"
                                        vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="20.0"
                                        vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Label GridPane.rowIndex="2" prefHeight="34.0" prefWidth="927.0"
                                        text="Procedure" textFill="WHITE" fx:id="procedureLabel">
                                    <font>
                                        <Font size="28.0"/>
                                    </font>
                                    <GridPane.margin>
                                        <Insets left="40.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label GridPane.rowIndex="1" prefHeight="49.0"
                                        prefWidth="916.0" textFill="WHITE" fx:id="procedureSummaryLabel">
                                    <GridPane.margin>
                                        <Insets left="40.0"/>
                                    </GridPane.margin>
                                    <font>
                                        <Font name="System Bold" size="40.0"/>
                                    </font>
                                </Label>
                                <TextField GridPane.rowIndex="1" maxWidth="888.0" prefHeight="25.0"
                                        prefWidth="888.0" fx:id="summaryEntry">
                                    <GridPane.margin>
                                        <Insets left="40.0"/>
                                    </GridPane.margin>
                                </TextField>
                            </children>
                        </GridPane>
                    </children>
                </TilePane>
                <GridPane style="-fx-background-color: #e3e3e5;" GridPane.columnSpan="2"
                        GridPane.rowIndex="1" GridPane.rowSpan="3">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="251.0" minWidth="10.0"
                                prefWidth="228.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="196.0" minWidth="10.0"
                                prefWidth="139.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="182.0" minWidth="10.0"
                                prefWidth="134.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints/>
                    </rowConstraints>
                    <children>
                        <Label GridPane.rowIndex="1" prefHeight="26.0" prefWidth="224.0"
                                text="Date:" fx:id="procedureDateLabel">
                            <font>
                                <Font size="22.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets left="20.0"/>
                            </GridPane.margin>
                        </Label>
                        <Label GridPane.columnSpan="6" GridPane.rowIndex="2" prefHeight="41.0"
                                prefWidth="1080.0" text="Organs Affected:"
                                fx:id="procedureOrgansLabel">
                            <font>
                                <Font size="22.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets bottom="40.0" left="20.0"/>
                            </GridPane.margin>
                        </Label>
                        <Label GridPane.columnSpan="4" GridPane.rowIndex="3"
                                GridPane.rowSpan="6" alignment="TOP_LEFT" prefHeight="179.0"
                                prefWidth="628.0" text="Description:" fx:id="procedureDescriptionLabel">
                            <font>
                                <Font size="22.0"/>
                            </font>
                            <GridPane.margin>
                                <Insets bottom="20.0" left="20.0"/>
                            </GridPane.margin>
                        </Label>
                        <ToolBar GridPane.rowIndex="7" GridPane.rowSpan="2" maxWidth="261.0"
                                prefHeight="59.0" prefWidth="251.0">
                            <items>
                                <Button style="-fx-background-image: url(icons/undo.png); -fx-background-size: stretch;" mnemonicParsing="false"
                                        onAction="#handleUndoButtonClicked" prefHeight="50.0"
                                        prefWidth="50.0"
                                        fx:id="undoButton"/>
                                <Button style="-fx-background-image: url(icons/redo.png); -fx-background-size: stretch;" layoutX="10.0" layoutY="10.0"
                                        mnemonicParsing="false" onAction="#handleRedoButtonClicked"
                                        prefHeight="50.0" prefWidth="50.0"
                                        fx:id="redoButton"/>
                                <Button style="-fx-background-image: url(icons/edit.png); -fx-background-size: stretch;" layoutX="64.0" layoutY="10.0"
                                        mnemonicParsing="false" onAction="#handleEditButtonClicked"
                                        prefHeight="50.0" prefWidth="50.0"
                                        fx:id="editButton"/>
                            </items>
                            <GridPane.margin>
                                <Insets bottom="35.0" top="20.0"/>
                            </GridPane.margin>
                        </ToolBar>
                        <TextArea GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="4"
                                GridPane.rowSpan="2" prefHeight="125.0"
                                prefWidth="397.0" fx:id="descEntry">
                            <GridPane.margin>
                                <Insets top="15.0"/>
                            </GridPane.margin>
                        </TextArea>
                        <Button GridPane.columnIndex="3" GridPane.rowIndex="7"
                                mnemonicParsing="false" onAction="#handleSaveButtonClicked"
                                prefHeight="35.0" prefWidth="105.0" text="Save"
                                fx:id="saveButton">
                            <GridPane.margin>
                                <Insets left="35.0"/>
                            </GridPane.margin>
                        </Button>
                        <ListView GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="2"
                                GridPane.rowSpan="2" prefHeight="61.0"
                                prefWidth="330.0" fx:id="affectedOrgansListView"/>
                        <DatePicker GridPane.columnIndex="1" GridPane.columnSpan="3"
                                GridPane.rowIndex="1" prefHeight="26.0"
                                prefWidth="426.0" promptText="Date of Procedure"
                                fx:id="dateOfProcedureDatePicker"/>
                        <Label GridPane.columnIndex="3" GridPane.rowIndex="6" prefHeight="15.0"
                                prefWidth="139.0" text="Invalid Date" textAlignment="CENTER"
                                textFill="RED" fx:id="warningLabel"/>
                    </children>
                </GridPane>
            </children>
        </GridPane>
    </children>
</AnchorPane>
