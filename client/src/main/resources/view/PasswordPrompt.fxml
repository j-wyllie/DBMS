<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane focusTraversable="true" prefHeight="201.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="odms.view.profile.PasswordPrompt">
    <Label layoutX="14.0" layoutY="14.0" text="First time logging in. Please set a password" />
    <PasswordField fx:id="passwordField" layoutX="12.0" layoutY="41.0" prefHeight="29.0" prefWidth="339.0" promptText="Enter Password" />
    <PasswordField fx:id="confirmPasswordField" layoutX="12.0" layoutY="87.0" prefHeight="29.0" prefWidth="339.0" promptText="Confirm Password" />
    <Label fx:id="errorLabel" layoutX="25.0" layoutY="125.0" text="Invalid Passwords. Must be at least 5 characters." textFill="RED" visible="false" />
    <Button layoutX="118.0" layoutY="141.0" mnemonicParsing="false" onAction="#handleConfirmBtnPressed" prefHeight="42.0" prefWidth="127.0" text="Save Password" />
</AnchorPane>
